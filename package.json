{
  "name": "@gerhobbelt/markdown-it-prism",
  "version": "1.0.1-2",
  "description": "Highlights code blocks in markdown-it using Prism.",
  "keywords": [
    "markdown-it",
    "markdown-it-plugin",
    "highlight",
    "prism",
    "prismjs"
  ],
  "homepage": "https://github.com/jGleitz/markdown-it-prism",
  "license": "GPL-3.0",
  "author": {
    "name": "Joshua Gleitze",
    "url": "https://joshuagleitze.de"
  },
  "files": [
    "README.md",
    "LICENSE",
    "build/index.js"
  ],
  "main": "build/index.js",
  "engines": {
    "node": ">=4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jGleitz/markdown-it-prism.git"
  },
  "scripts": {
    "build": "babel *.js -d build",
    "lint": "eslint *.js",
    "prepublish-orig": "npm run build",
    "pretest": " npm run build",
    "unittest": "mocha build/test.js",
    "test": "npm run lint && npm run unittest",
    "pub": "npm publish --access public"
  },
  "dependencies": {
    "prismjs": "1.15.0"
  },
  "devDependencies": {
    "@gerhobbelt/babel-cli": "7.2.1-26",
    "@gerhobbelt/babel-core": "^7.2.1-26",
    "@gerhobbelt/babel-plugin-add-module-exports": "0.3.3-1",
    "@gerhobbelt/debug": "^4.1.1-4",
    "@gerhobbelt/markdown-it": "8.4.2-24",
    "chai": "4.2.0",
    "chai-string": "1.5.0",
    "eslint": "5.12.0",
    "mocha": "5.2.0",
    "slash": "^2.0.0"
  },
  "babel": {
    "plugins": [
      "@gerhobbelt/babel-add-module-exports"
    ]
  }
}
