{
  "name": "@gerhobbelt/markdown-it-prism",
  "version": "2.1.0-5",
  "description": "Highlights code blocks in markdown-it using Prism.",
  "keywords": [
    "markdown-it",
    "markdown-it-plugin",
    "highlight",
    "prism",
    "prismjs"
  ],
  "homepage": "https://github.com/jGleitz/markdown-it-prism",
  "license": "GPL-3.0",
  "author": {
    "name": "Joshua Gleitze",
    "url": "https://joshuagleitze.de"
  },
  "files": [
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "build/index.js",
    "build/index.d.ts"
  ],
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jGleitz/markdown-it-prism.git"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "build:transpile": "babel ./src --out-dir build --extensions '.ts'",
    "build:types": "tsc --project src",
    "lint": "npm-run-all lint:*",
    "lint:types": "tsc",
    "lint:style": "eslint .",
    "lint:commits": "commitlint --from=semantic-commit-start --to=HEAD",
    "prepack": "npm-run-all build:*",
    "unittest": "jest",
    "test": "npm-run-all lint:* unittest",
    "pub": "npm publish --access public"
  },
  "peerDependencies": {
    "@gerhobbelt/prismjs": "1.*"
  },
  "devDependencies": {
    "@babel/cli": "7.14.5",
    "@babel/core": "7.14.6",
    "@babel/preset-env": "7.14.7",
    "@babel/preset-typescript": "7.10.4",
    "@babel/register": "7.14.5",
    "@gerhobbelt/markdown-it": "12.0.6-55",
    "@semantic-release/git": "9.0.0",
    "@types/jest": "26.0.4",
    "@types/markdown-it": "10.0.1",
    "@types/prismjs": "1.16.1",
    "@typescript-eslint/eslint-plugin": "3.6.0",
    "@typescript-eslint/parser": "3.6.0",
    "babel-plugin-add-module-exports": "1.0.4",
    "eslint": "7.29.0",
    "jest": "26.1.0",
    "mocha": "8.0.1",
    "@gerhobbelt/prismjs": "1.23.0-4",
    "semantic-release": "17.1.1",
    "typescript": "3.9.6"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/changelog",
        {
          "changelogTitle": "# [Changelog](http://keepachangelog.com/)\n\nThis project adheres to [Semantic Versioning](http://semver.org/)."
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ]
        }
      ]
    ]
  }
}
